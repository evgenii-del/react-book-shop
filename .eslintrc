{
  "extends": ["airbnb", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/prop-types": 0,
    "no-unused-vars": "warn",
    "prettier/prettier": [
      "error",
      { "endOfLine": "auto" },
      {
        "react/jsx-filename-extension": [
          1,
          {
            "extensions": [".js", ".jsx"]
          }
        ]
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "react/jsx-props-no-spreading": 0,
    "no-undef": 0,
    "no-console": 0
  }
}
